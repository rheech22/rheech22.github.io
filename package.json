{
  "name": "gatsby-starter-blog-github-theme",
  "version": "1.0.0",
  "description": "inspired github's styles",
  "author": "rheech22",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby clean && gatsby build && gh-pages -d public -b master",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint --fix --ext .ts,.tsx ."
  },
  "dependencies": {
    "@deckdeckgo/highlight-code": "^4.3.0",
    "@giscus/react": "^3.1.0",
    "@loadable/component": "^5.15.3",
    "@react-hook/window-size": "^3.1.1",
    "babel-plugin-styled-components": "^2.0.7",
    "gatsby": "^5.11.0",
    "gatsby-plugin-catch-links": "^5.11.0",
    "gatsby-plugin-google-gtag": "^5.11.0",
    "gatsby-plugin-image": "^3.11.0",
    "gatsby-plugin-manifest": "^5.11.0",
    "gatsby-plugin-offline": "^6.11.0",
    "gatsby-plugin-sharp": "^5.11.0",
    "gatsby-plugin-sitemap": "^6.11.0",
    "gatsby-plugin-styled-components": "^6.11.0",
    "gatsby-remark-autolink-headers": "^6.11.0",
    "gatsby-remark-copy-linked-files": "^6.11.0",
    "gatsby-remark-highlight-code": "^3.3.0",
    "gatsby-remark-images": "^7.11.0",
    "gatsby-source-filesystem": "^5.11.0",
    "gatsby-transformer-remark": "^6.11.0",
    "gatsby-transformer-sharp": "^5.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph-3d": "^1.23.0",
    "react-resize-detector": "^12.0.2",
    "sharp": "^0.33.5",
    "styled-components": "^6.0.7",
    "styled-reset": "^4.5.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^20.17.14",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.16",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "ajv": "^8.17.1",
    "babel-preset-gatsby": "^3.11.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "gatsby-plugin-eslint": "^4.0.2",
    "gh-pages": "^4.0.0",
    "prettier": "^3.0.1",
    "typescript": "^5.7.3"
  },
  "importSort": {
    ".js, .jsx, .es6, .es, .mjs, .ts, .tsx": {
      "parser": "babylon",
      "style": "import-sort-style-module"
    }
  },
  "overrides": {
    "babel-plugin-lodash": {
      "@babel/types": "~7.20.0"
    }
